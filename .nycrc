{
  "check-coverage": true,
  "statements": 80,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "text-summary",
    "teamcity",
    "html"
  ],
  "sourceMap": true,
  "instrument": true
}